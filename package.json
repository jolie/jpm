    },
    {
      "name": "Narongrit Unwerawattana",
      "email": "narongrit.kie@gmail.com"
    }
  ],
  "bin": {
    "jpm": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jolie/jpm.git"
  },
  "keywords": [
    "jolie",
    "microservices"
  ],
  "homepage": "https://github.com/jolie/jpm",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^5",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "decompress": "^4.2.1",
    "glob": "^8.0.3",
    "mime-types": "^2.1.35",
    "node-fetch": "^2.6.7",
    "pom-parser": "1.2.0",
    "semver": "^7.3.7",
    "shelljs": "^0.8.5",
    "xml": "^1.0.1"
  },
  "overrides": {
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@oclif/test": "^4",
    "@tsconfig/node16-strictest-esm": "^1.0.1",
    "@types/chai": "^4",
    "@types/debug": "^4.1.7",
    "@types/decompress": "^4.2.4",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.36",
    "@types/node-fetch": "^2.6.1",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.11",
    "@types/xml": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "chai": "^4",
    "eslint": "^8.16.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "globby": "^11",
    "mocha": "^9",
    "nock": "^13.2.4",
    "oclif": "^4.10.6",
    "prettier": "2.6.2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "jpm",
    "dirname": "jpm",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist tsconfig.tsbuildinfo && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "lint-fix": "eslint . --fix --ext .ts --config .eslintrc.json",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/kicito/jpm/issues",
  "types": "dist/index.d.ts",
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
