{
  "name": "@jolie/jpm",
  "version": "3.0.3",
  "description": "The Jolie package manager",
  "contributors": [
    {
      "name": "Luis Antonio Canettoli OrdoÃ±ez",
      "email": "hello@luisanton.io"
    },
    {
      "name": "Fabrizio Montesi",
